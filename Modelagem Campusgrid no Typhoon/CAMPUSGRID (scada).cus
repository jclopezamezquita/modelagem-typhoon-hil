# Schematic Editor model namespace is imported as 'scm'
# Function for printing to HIL SCADA Message log is imported as 'printf'.


SETTING_DIR = get_settings_dir_path()

counter = 0
counter2 = 0
counter3 = 0
# wait = 4 #1800 #load profile waiting time in seconds
Data = [['P']]

simulationStep = hil.get_sim_step()
print(simulationStep)

wait = simulationStep * 2000000

import numpy as np

# Reads csv files and put results in the array
# This function actually reads any txt file

def read_csv(file_path):
	lookup_file = open(file_path, 'r')
	calc_res = lookup_file.readlines()
	lookup_file.close()
	
	calc_res = np.array(calc_res).astype(float)
	
	return calc_res
    
# write into the file

def write_csv(file_path, data_array):
	
	file = open(file_path, 'w')
	for i in range(len(data_array)):
		file.write(str(data_array[i]) + '\n')
		
		file.close()
        
# Print the path used for reading and saving files
printf(SETTING_DIR)

# example of calling the read_csv function
file_data = read_csv(SETTING_DIR + '/pload_profile.txt')
file_data2 = read_csv(SETTING_DIR + '/qload_profile.txt')
file_data3 = read_csv(SETTING_DIR + '/solar_irradiation.txt')]]></initialization_code>
